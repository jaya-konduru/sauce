{
  "name": "Jaya/sauce/sausage",
  "type": "sauce-sausage",
  "description": "PHP version of the Sauce Labs API",
  "keywords": ["sauce", "saucelabs", "api", "testing", "phpunit", "selenium", "appium"],
  "homepage": "https://github.com/jaya-konduru/sauce",
  "license": "Apache-2.0",
  "authors": [{
    "name": "Jayachandra",
    "email": "jayak8390@gmail.com",
    "homepage": "http://www.saucelabs.com",
  }],
  "repositories": [
      {
          "type": "vcs",
          "url": "https://github.com/appium/php-client"
      }
  ],
  "require-dev": {
    "phpunit/phpunit": ">=5.3.2"
  },
  "require": {
    "php": ">=5.6",
    "sauce/sausage-installer": ">=0.1.0",
    "phpunit/phpunit-selenium": ">=3.0.2",
    "brianium/paratest": ">=0.13.2",
    "appium/php-client": ">=0.1.0"
  },
  "suggest": {
    "sauce/connect": ">=3.1"
  },
  "autoload": {
    "psr-0": {"Sauce": "src/"}
  },
  "bin": ["bin/sauce_config"]
}